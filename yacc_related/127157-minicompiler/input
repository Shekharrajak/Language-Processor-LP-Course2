
int main()
{
	int x,y,z=4;
    int a;
	x=y;
	y=7;
	z=3;
    z*=5;
    while(y+z<=20)
    {
    	y=y+1;
    	z=x+2*5+(4+7)*(5/y);
    	switch(a)
    	{
    		case 1 : y=1;
    				 break;
    		case 2 : y=2;
    		default : y=x;
    	}
    }
    a=5;
    if(x<2)
    {
       x=x+5;
    }
    else
    {
       x=x-5;
    }


}